section	.payload_terrain_physical_map		load	FFA3
section	.payload_terrain			load	6000	; $16,$17,$18

section	.payload_level_physical_map		load	FFA4
section	.payload_level				load	8000	; $15


section .payload_terrain_offset_physical_map	load	FFA5
section	.payload_terrain_offset			load	A000	; $16

section	.program_code_physical_map		load	FFA6
section	program_code				load	C000	; $30

section module_gfx
section module_drawterrain
section module_interrupt
section .bss

section	.program_code_stack			load	FD00

entry	ProgramCode
